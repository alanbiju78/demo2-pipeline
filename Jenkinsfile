pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello world'
            }
        }

        stage('Build') {
            steps {
                echo 'Building the application'
            }
        }

        stage('Prepare') {
            steps {
                echo 'Preparing the application'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Testing the application'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Deploying the application'
            }
        }
    }
}
